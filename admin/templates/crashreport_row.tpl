<div class="report_item">
  <div class="productid application [@iconclass]">[@productid]<span class="version">[@version]</span></div>
  <div class="timestamp">[@timestamp]</div>
  <div class="stacktrace_container">
    <div class="stacktrace" onclick="javascript:onClickStackTrace(this);">[@stacktrace]</div>
  </div>
  <div class="spacer"></div>
  <div class="description" title="[@description]">[@description]</div>
  <div class="ip">[@ipstr]</div>
  <div class="icon_center">
    <div class="icon_container">
      <div title="Download" class="download icon" onclick="javascript:download('[@dumpfile]');"></div>
      <div title="Delete" class="delete icon" onclick="javascript:deleteEntryWithConfirm([@entryid]);"></div>
    </div>
  </div>
</div>